{
	"name": "dcarrith/cizip",
	"description": "A port of the zip library for CodeIgniter",
	"keywords": ["compress", "compression", "zip", "cizip"],
    	"homepage": "http://github.com/dcarrith/cizip",
    	"type": "library",
    	"license": "MIT",
    	"authors": [
        	{
        		"name": "David Carrithers",
            		"email": "dcarrith@gmail.com",
            		"homepage": "http://github.com/dcarrith"
        	}
    	],
	"repositories": [
        	{
        		"type": "vcs",
        		"url": "https://github.com/dcarrith/cizip"
        	}
    	],
    	"require": {
        	"php": ">=5.4.0",
        	"illuminate/support": "4.*"
    	},
    	"autoload": {
        	"classmap": [
            		"src/migrations"
        	],
        	"psr-0": {
            		"Dcarrith\\CIZip": "src/"
        	}
    	},
    	"extra": {
    		"branch-alias": {
    			"dev-master": "1.0-dev"
    		}
    	},
	"minimum-stability": "dev"
}
