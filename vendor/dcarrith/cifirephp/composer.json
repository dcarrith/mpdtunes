{
	"name": "dcarrith/cifirephp",
	"description": "A wrapper class for backwards compatibility with the old FirePHP library that was available for CodeIgniter",
	"keywords": ["log", "logging", "firephp", "cifirephp"],
    	"homepage": "http://github.com/dcarrith/cifirephp",
    	"type": "library",
    	"license": "MIT",
    	"authors": [
        	{
        		"name": "David Carrithers",
            		"email": "dcarrith@gmail.com",
            		"homepage": "http://github.com/dcarrith"
        	}
    	],
	"repositories": [
        	{
        		"type": "vcs",
        		"url": "https://github.com/dcarrith/cifirephp"
        	}
    	],
    	"require": {
        	"php": ">=5.4.0",
        	"illuminate/support": "4.*",
		"monolog/monolog": "1.*"
    	},
    	"autoload": {
        	"classmap": [
            		"src/migrations"
        	],
        	"psr-0": {
            		"Dcarrith\\CIFirePHP": "src/"
        	}
    	},
    	"extra": {
    		"branch-alias": {
    			"dev-master": "1.0-dev"
    		}
    	},
	"minimum-stability": "dev"
}
